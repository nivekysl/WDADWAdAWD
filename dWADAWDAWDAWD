	game:GetService("ReplicatedStorage").Connection:InvokeServer(79)
end)

meep:AddSwitch("Loop Toggle Meep",loopwrap(function()
	wait(1.3)
	game:GetService("ReplicatedStorage").Connection:InvokeServer(79)
end))

